{
	"compileOnSave": false,
	"strictNullChecks": true,
	"compilerOptions": {
	  "noUnusedLocals": false,
	  "noUnusedParameters": false,
	  "declaration": true,
	  "removeComments": true,
	  "module": "Node16",
	  "target": "ES6",
	  "moduleResolution": "Node16",
	  "noImplicitAny": false,
	  "types": [
		"node",
	  ],
	  "outDir": "dist",
	  "esModuleInterop": true,
	  "resolveJsonModule": true
	},
	"include": ["./src", "../shared", "./global.d.ts"]
}  